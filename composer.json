{
  "name": "b2brouter/b2brouter-php",
  "description": "B2Brouter PHP SDK - Official library for the B2Brouter API",
  "keywords": [
    "b2brouter",
    "api",
    "invoice",
    "electronic documents",
    "sdk"
  ],
  "homepage": "https://developer.b2brouter.net",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "B2Brouter",
      "email": "servicedelivery@b2brouter.net"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "B2BRouter\\": "lib/B2BRouter/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "B2BRouter\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
